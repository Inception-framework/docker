FROM ubuntu:16.04
MAINTAINER Corteggiani Nassim nassim.corteggiani@maximintegrated.com
MAINTAINER Camurai Giovanni giovanni.camurati@eurecom.fr

ARG MAKE_JOBS=16
RUN echo "building using $MAKE_JOBS jobs"

#
#
# Prepare system by installing required packages
#
#
RUN apt-get update && apt-get install -y --force-yes software-properties-common git \
	git make binutils-dev \
	flex bison pkg-config wget nano \
	binutils-arm-none-eabi binutils binutils-dev build-essential\
	python3.5-dev python3-pip\
	sudo libc++-dev libc++1 libedit-dev groff \
	cmake make pkg-config autoconf automake libusb-1.0 usbutils \
	curl libcap-dev libncurses5-dev python-minimal python-pip unzip libusb-1.0-0-dev \
	libboost-all-dev perl zlib1g-dev libnewlib-arm-none-eabi libjsoncpp-dev libjsoncpp1 \
        vim tig

RUN apt-get update && apt-get install -y --force-yes minicom
RUN apt-get update && apt-get install -y --force-yes libc6-dev-i386

RUN apt-get clean \
	&& rm -rf /var/lib/apt/lists/*

#
#
# Fix Ubuntu issues
#
#
#Ubuntu stores json headers into jsoncpp/json but we want json
RUN ln -s /usr/include/jsoncpp/json /usr/include/json

#
#
# Configure system to enable USB device access
#
#
#
RUN mkdir -p /etc/udev/rules.d \
&& touch /etc/udev/rules.d/88-cyusb.rules \
&& echo 'KERNEL=="*", SUBSYSTEM=="usb", ENV{DEVTYPE}=="usb_device", ACTION=="add", ATTR{idVendor}=="04b4", MODE="0666"' \
| tee --append /etc/udev/rules.d/88-cyusb.rules

RUN echo ''\
'# This is the main configuration file for Cypress USB FX2 and FX3 devices\n'\
'# All lines beginning with a # in the first column is treated as a comment     \n'\
'# Blank lines are ignored                                                      \n'\
'# Author    - V. Radhakrishnan ( www.atr-labs.com )                            \n'\
'# Copyright - ATR Educational Devices (P) Ltd and Cypress Semiconductors Inc.  \n'\
'# Date      - March 7, 2012                                                    \n'\
'# License   - GPL Version 2                                                    \n'\
'#---------------------------------------------------------------------------   \n'\
'# Location of logfile for the Cypress USB daemon process 'cyusbd'              \n'\
'# Ensure that you have write permission in that directory, if you are non-root.\n'\
'LogFile=cyusbd.log                                                             \n'\
'# Location of PID file for the Cypress USB daemon process 'cyusbd'             \n'\
'# Ensure that you have write permission in that directory, if you are non-root.\n'\
'PIDFile=cyusbd.pid                                                             \n'\
'# Vital Product Data : Vendor/Device IDs - one per line.                       \n'\
'# Format - vendorID	DeviceID	FriendlyName (Max 30 chars or end of line)      \n'\
'<VPD>                                                                          \n'\
'04b4	8613		Default Cypress USB device                                        \n'\
'04b4	4717		Default Cypress USB device                                        \n'\
'04b4	0082		FX2 Device: Custom PID from EEPROM                                \n'\
'04b4	1004		FX2LP Bulk Device                                                 \n'\
'04b4	1003		FX2LP CY-Stream Device                                            \n'\
'04b4	00BC		Cypress Benicia device                                            \n'\
'04b4	00F0		FX3 Example Application                                           \n'\
'04b4	00F1		FX3 Streamer Example                                              \n'\
'04b4	00F3		FX3 Boot-loader                                                   \n'\
'04b4	4720		FX3 I2C/SPI Programmer                                            \n'\
'04b4	1005		FX3 2nd stage boot-loader                                         \n'\
'</VPD>                                                                         \n'\ | tee --append /etc/cyusb.conf


RUN touch /etc/ \
&& echo ''\
'#!/bin/bash               \n'\
'pid=`pidof cyusb_linux`   \n'\
'if [ "$pid" ]; then       \n'\
'    kill -s SIGUSR1 $pid  \n'\
'fi                        \n' | tee --append /usr/local/bin/cy_renumerate.sh \
&& chmod 777 /usr/local/bin/cy_renumerate.sh


# Remove stale versions of the libcyusb library
RUN rm -f /usr/lib/libcyusb.so* /usr/local/lib/libcyusb.so* /usr/lib/x86_64-linux-gnu/libcyusb.so*

# Copy the libcyusb library into the system library folders.
