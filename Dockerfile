#
#
# Generate directories tree structure
#
#
RUN mkdir /home/inception \
&& mkdir /home/inception/Analyzer \
&& mkdir /home/inception/compiler \
&& mkdir /home/inception/rt-debugger-driver \
&& mkdir /home/inception/samples \
&& mkdir /home/inception/stubs \
&& mkdir /home/inception/tools \
&& mkdir /home/inception/tools/llvm \
&& mkdir /home/inception/tools/llvm/build_debug \
&& mkdir /home/inception/tools/llvm/build_release
RUN mkdir /home/inception/tools/mbed-cli

#
#
# Configure git, system user and docker workdir
#
#
RUN useradd -ms /bin/bash inception \
&& echo "inception:inception" | chpasswd && adduser inception sudo \
&& chown -R inception:inception /home/inception
WORKDIR /home/inception

USER inception

#
#
# Clone Inception modules
#
#
RUN git clone --branch master https://gitlab.eurecom.fr/nasm/Inception-analyzer.git ./Analyzer \
&& git clone --branch master https://gitlab.eurecom.fr/nasm/Inception-compiler.git ./compiler \
&& git clone --branch master https://gitlab.eurecom.fr/nasm/Inception-debugger-driver.git ./rt-debugger-driver \
&& git clone --branch master https://gitlab.eurecom.fr/nasm/Inception-samples.git ./samples


#
#
# Downloads and install dependencies that are not available as public packages
#
#
# Download LLVM 3.6.2 that is the version supported by Inception
WORKDIR /home/inception/tools/llvm
RUN git clone https://github.com/llvm-mirror/llvm llvm3.6 \
&& cd llvm3.6 \
&& git checkout release_36

# Download Clang 3.6.2 that is the version supported by Inception
RUN cd llvm3.6/tools \
&& git clone https://github.com/llvm-mirror/clang \
&& cd clang && git checkout release_36

# Build LLVM with Clang
RUN cd build_debug \
&& ../llvm3.6/configure --enable-debug-symbols --build=x86_64-linux-gnu \
&& make -j${MAKE_JOBS}

# Install llvm in /usr/local/lib
USER root
RUN cd build_debug && make install
USER inception

# Install mbed cli
USER root
RUN git clone https://github.com/ARMmbed/mbed-cli.git ./mbed-cli
RUN cd ./mbed-cli && python setup.py install
USER inception

WORKDIR /home/inception/tools

#Installing Analyzer (Klee) dependencies
#Installing Minisat
RUN git clone https://github.com/stp/minisat.git ./minisat \
&& cd minisat \
&& mkdir build \
&& cd build \
&& cmake -DSTATIC_BINARIES=ON -DCMAKE_INSTALL_PREFIX=/usr/ ../  \
&& make all

# Install llvm in /usr/local/lib
USER root
RUN cd minisat/build && make install
USER inception

#Installing STP
RUN git clone https://github.com/msoos/cryptominisat ./cryptominisat \
&& cd cryptominisat  \
&& mkdir build  \
&& cd build  \
&& cmake ..  \
&& make

# Install llvm in /usr/local/lib
USER root
RUN cd cryptominisat/build && make install && ldconfig
USER inception

#Installing STP
RUN git clone https://github.com/stp/stp.git stp \
&& cd stp \
&& git checkout tags/2.1.2 \
&& mkdir build \
&& cd build \
&& cmake -DBUILD_SHARED_LIBS=OFF -DENABLE_PYTHON_INTERFACE=OFF .. \
&& make \
&& ulimit -s unlimited

USER root
RUN cd stp/build && make install
USER inception

#Installing klee-uclibc
RUN git clone https://github.com/klee/klee-uclibc.git ./klee-uclibc \
&& cd klee-uclibc \
&& ./configure --with-llvm-config ../llvm/build_debug/Debug+Asserts/bin/llvm-config --make-llvm-lib \
&& make -j${MAKE_JOBS}

#
#
# Install Inception modules
#
#
WORKDIR /home/inception

#Installing Inception-driver
# This is the last stable commit before moving to cmake
RUN cd rt-debugger-driver && git checkout 7524f1371f16fb8230834090d2697e35d1d45018
RUN  cd ./rt-debugger-driver/Debug/ \
&& make all \
&& find . -name "*.o" -and -not -name "main.o" -print0 | xargs -0 ar crv libinception.a

USER root
RUN cd ./rt-debugger-driver/Debug \
&& GCC_VERSION="$(gcc -dumpversion)" \
&& cp libinception.a /usr/lib/gcc/x86_64-linux-gnu/$GCC_VERSION/libinception.a
USER inception

#Installing Analyzer
ARG LLVM_VERSION=3.6
RUN cd ./Analyzer \
&& DIR="$(cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )" \
&& ./autoconf/AutoRegen.sh ../tools/llvm/llvm3.6/ \
&& CXXFLAGS="-g3 -O0 -fexceptions" CFLAGS="-g3 -O0" ./configure \
            --enable-cxx11 \
            --with-stp=$DIR/../tools/stp/ \
            --enable-posix-runtime \
            --with-llvmsrc=$DIR/../tools/llvm/llvm$LLVM_VERSION \
            --with-llvmobj=$DIR/../tools/llvm/build_debug \
            --with-llvmcc=$DIR/../tools/llvm/build_debug/Debug+Asserts/bin/clang \
            --with-llvmcxx=$DIR/../tools/llvm/build_debug/Debug+Asserts/bin/clang++ \
            --with-runtime='Debug+Asserts' \
	    --with-llvm-build-mode='Debug+Asserts' \
&& make -j${MAKE_JOBS} all

USER root
RUN cd ./Analyzer && make install
USER inception

# This is the current working commit with the cmake toolchain
RUN mkdir ./compiler/build
RUN cd ./compiler/build && cmake ../ && make -j${MAKE_JOBS} 

USER root
RUN cd ./compiler/build && make install
USER inception

RUN echo "Credits :" \
&& echo "Author : G. Camurati and N. Corteggiani" \
&& echo "ID     : Inception" \
&& echo "Year   : 2016/2017" \
&& echo "Status : Release" \
&& echo " Enjoy !"


CMD ["echo","Home directory $HOME"]
CMD ["echo","Workspace directory $HOME/projects"]
CMD ["echo","Welcome to the Inception framework !"]
